{
    "name": "rabus/slb2019-workshop",
    "description": "Demo Application for SymfonyLive Berlin 2019",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Alexander M. Turek",
            "email": "me@derrabus.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3.9",
        "derrabus/ereg-shim": "^1.0",
        "sensio/framework-extra-bundle": "^5.4",
        "shardj/zf1-future": "^1.14",
        "symfony/asset": "^4.3",
        "symfony/console": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/flex": "^1.4",
        "symfony/framework-bundle": "^4.3",
        "symfony/monolog-bundle": "^3.4",
        "symfony/twig-bundle": "^4.3",
        "symfony/yaml": "^4.3",
        "tecnickcom/tcpdf": "^6.3"
    },
    "config": {
        "platform": {
            "php": "7.3.9"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-0": {
            "TravelOrganizer": "library"
        },
        "psr-4": {
            "TravelOrganizer\\": "src"
        },
        "classmap": [
            "application"
        ]
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "symfony/debug-bundle": "^4.3",
        "symfony/profiler-pack": "^1.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
